    <sensor name="wrist_rgbd_depth_sensor" type="depth">
      <always_on>true</always_on>
      <update_rate>15.0</update_rate>
      <camera>
        <!--<horizontal_fov>${91.2*M_PI/180.0}</horizontal_fov>-->
        <!--<horizontal_fov>${91.2*M_PI/340.0}</horizontal_fov>-->
