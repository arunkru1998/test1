        <image>
          <format>R8G8B8</format>
          <width>640</width>
          <height>480</height>
        </image>
        <clip>
          <near>0.01</near>
          <far>1.0</far>
        </clip>
      </camera>
      <plugin filename="libgazebo_ros_camera.so" name="wrist_rgbd_controller">
        <cameraName>front_rgbd_camera</cameraName>
